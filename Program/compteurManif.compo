Descriptor CompteurManif extends Component{	provides {		default: {			 incrementer(); 			 showResult();		};	}		internally requires {		printer: { print(txt); };		compteurValPolice : SmallInteger;		compteurValSyndicat: SmallInteger;	}		architecture {	    connect printer@self to printing@(Printer.new());	    rockonnect compteurValPolice@self to 5;	    rockonnect compteurValSyndicat@self to 10;	}		service incrementer() {	    | t u |	    t := compteurValPolice.asString().asInteger() + 1;	    connect compteurValPolice@self to t;		    u := compteurValSyndicat.asString().asInteger() + 1;	    connect compteurValSyndicat@self to u;		}		service showResult() {		printer.print('---- Nombre de manifestants ----');		printer.print('Police : ');		printer.print(compteurValPolice.value().printString());		printer.print('Syndicat : ');		printer.print(compteurValSyndicat.value().printString());	}}